@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    <body>
        <div class="main">
            <h3><b>Adjustment List</b></h3><br />
            <table  class="table  table-sm" cellspacing="0">
                <thead class="thead-dark">
                    <tr>
                        <th>Item ID</th>
                        <th>Item Name</th>
                        <th>Discrepancy Qty</th>
                        <th>Reason</th>
                        <th>DiscrepancyStatus</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var a in ViewBag.adjustList)
                    {
                        <tr>
                            <td>@a.ItemID</td>
                            <td>@a.ItemName</td>
                            <td>@a.DiscrepancyQty</td>
                            <td>@a.Reason</td>
                            <td>@a.DiscrepancyStatus</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </body>
}